#!/usr/bin/env bash
# This script is displaying "Holberton School" 10 times.

x=0
while [ $x -le 100 ]; do
    if [ $(( x % 3 )) -eq 0 -a $(( x % 5 )) -eq 0 ]
       then
	   echo "FizzBuzz"
    elif [ $(( x % 3 )) -eq 0 ]
       then
	   echo "Fizz"
    elif [ $(( x % 5 )) -eq 0 ]
       then echo "Buzz"
    else
	echo $x
    fi
    x=$(( x + 1 ))
done
